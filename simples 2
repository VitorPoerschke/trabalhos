
#include <math.h>
#include <stdio.h>


int main(){
	
	
	int ano;
	int atual;
	
	printf("\nDigite a sua data de nascimento: ");
	scanf("%i",&ano);
	
	printf("\nAgora digite o ano atual: ");
	scanf("%i",&atual);
	
	int idade = atual - ano;
	
	printf("\nidade ser: %i",idade);
	
	int tempo;
	int i;
	
	for(i=atual; i<=2035; i++){
		(tempo = i);
	}
	
	int resto = tempo - atual;
	
	int sobra = resto + idade;
	
	printf("\nA idade em 2035 ser: %i",sobra);
	
}






